<?xml version='1.0' encoding='utf-8'?>
<widget id="tech.buyqaw.mobileapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BuyQawHub</name>
    <description>
        This application implements BuyQaw HUB system.
    </description>
    <author href="http://buyqaw.tech">
        Bauyrzhan Ospan
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://buyqaw.tech" />
    <allow-intent href="https://buyqaw.tech" />
    <allow-intent href="tel:+77081993506" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:ceo@buyqaw.tech" />
    <allow-intent href="geo:Astana" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-ble-peripheral" />
    <splash src="splash.png" />
    <icon src="icon.png" />
</widget>
